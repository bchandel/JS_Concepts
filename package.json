    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^7.5.0",
    "mongoose-lean-getters": "^1.1.0",
    "mongoose-sequence": "^5.3.1",
    "winston": "^3.10.0"
